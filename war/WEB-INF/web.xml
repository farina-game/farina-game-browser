<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Farina_Game_Browser</servlet-name>
		<servlet-class>net.falsecam.farina_game_browser.Farina_Game_BrowserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>net.falsecam.farina_game_browser.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BrowserServlet</servlet-name>
		<jsp-file>/jsp/browser.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Farina_Game_Browser</servlet-name>
		<url-pattern>/farina_game_browser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BrowserServlet</servlet-name>
		<url-pattern>/browser.html</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
